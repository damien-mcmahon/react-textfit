{
  "name": "react-textfit",
  "version": "1.1.4",
  "description": "Typescript React component to fit headlines and paragraphs into any element",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel  ./src --out-dir lib --extensions '.ts,.tsx'",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch",
    "prepare": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/damien-mcmahon/react-textfit/"
  },
  "keywords": [
    "textfit",
    "fittext",
    "text",
    "react",
    "react-component"
  ],
  "author": "Damien McMahon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malte-wessel/react-textfit/issues"
  },
  "homepage": "https://github.com/malte-wessel/react-textfit",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-react": "^7.28.0",
    "expect": "^1.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^2.3.4",
    "source-map-loader": "^3.0.1",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "eslint-plugin-react": "^7.28.0"
  },
  "dependencies": {
    "process": "^0.11.10"
  }
}
